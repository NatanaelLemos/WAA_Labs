<?xml version='1.0' encoding='UTF-8'?>
<faces-config xmlns="http://xmlns.jcp.org/xml/ns/javaee"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee
        http://xmlns.jcp.org/xml/ns/javaee/web-facesconfig_2_2.xsd"
	version="2.2">

	<navigation-rule>
		<from-view-id>index.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>example1</from-outcome>
			<to-view-id>faces/hello.xhtml</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>example2</from-outcome>
			<to-view-id>faces/welcome.xhtml</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>example3</from-outcome>
			<to-view-id>faces/studentId.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>


	<navigation-rule>
		<from-view-id>/faces/hello.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>success</from-outcome>
			<to-view-id>/faces/welcome.xhtml</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>cancel</from-outcome>
			<to-view-id>/index.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>

	<navigation-rule>
		<from-view-id>/faces/welcome.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>success</from-outcome>
			<to-view-id>/faces/hello.xhtml</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>cancel</from-outcome>
			<to-view-id>/index.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>

	<navigation-rule>
		<from-view-id>/faces/studentId.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>result</from-outcome>
			<to-view-id>faces/studentId.xhtml</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>cancel</from-outcome>
			<to-view-id>/index.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>


</faces-config>